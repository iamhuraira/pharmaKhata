{
  "name": "pharmaKhata",
  "version": "3.55.4",
  "author": "Abu Huraira (https://github.com/iamhuraira429/)",
  "scripts": {
    "dev:spotlight": "spotlight-sidecar",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "build": "VERCEL_BUILD_CACHE=0 next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "prepare": "node scripts/prepare.js",
    "seed": "tsx src/seeders/index.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@eslint/eslintrc": "^3.1.0",
    "@playwright/test": "^1.50.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@sentry/nextjs": "^9.1.0",
    "@svgr/webpack": "^8.1.0",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@tanstack/react-query": "^5.54.1",
    "@tanstack/react-query-devtools": "^5.54.1",
    "@types/react": "^18.3.3",
    "antd": "^5.22.4",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cssnano": "^7.0.6",
    "dayjs": "^1.11.13",
    "envalid": "^8.1.0",
    "eslint-plugin-clsx": "^0.0.9",
    "express": "^5.1.0",
    "formik": "^2.4.6",
    "http-status-codes": "^2.3.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^26.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "next": "^15.0.3",
    "or": "^0.2.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-circular-progressbar": "^2.1.0",
    "react-day-picker": "^9.5.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-loading-skeleton": "^3.5.0",
    "react-redux": "^9.1.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-usestateref": "^1.0.9",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "swiper": "^11.1.15",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4",
    "use-file-picker": "^2.1.2",
    "yup": "^1.6.1",
    "zod": "^3.23.8",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@eslint-react/eslint-plugin": "^1.10.1",
    "@next/bundle-analyzer": "^14.2.5",
    "@next/eslint-plugin-next": "^14.2.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@spotlightjs/spotlight": "^2.10.3",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.1",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "checkly": "^4.8.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "semantic-release": "^24.1.0",
    "start-server-and-test": "^2.0.5",
    "storybook": "^8.2.9",
    "tsx": "^4.19.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "vitest-fail-on-console": "^0.7.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run build"
    }
  }
}
